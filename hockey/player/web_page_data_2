    <script>
        function toggleTeam(teamId) {
            document.getElementById(teamId).classList.toggle("hidden");
        }

        function showPlayerModal(playerId) {
            document.getElementById(playerId).classList.remove("hidden");
        }

        function closeModal(playerId) {
            document.getElementById(playerId).classList.add("hidden");
        }
    </script>
</body>
</html>
